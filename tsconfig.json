{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["./src/Components/*"],
      "@components/*": ["./src/Components/*"],
      "@routes/*": ["./src/Routes/*"],
      "@Screens/*": ["./src/Screens/*"],
      "@storage/*": ["./src/Storage/*"],
      "@theme/*": ["./src/Theme/*"],
      "@utils/*": ["./src/Utils/*"],
      "@Hooks/*": ["./src/Hooks/*"]
    }
  }
}
